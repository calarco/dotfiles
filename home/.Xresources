Xcursor.theme: Deepin-Cursor
Xcursor.size:  22

! urxvt
URxvt.background: rgba:2400/2c00/3000/dddd
URxvt.foreground: #999993
URxvt.cursorColor: #395573
URxvt.borderColor: rgba:2400/2c00/3000/dddd
URxvt.colorUL: #4A90D9
URxvt.cursorBlink: true
URxvt.borderless: false
URxvt.internalBorder: 10
URxvt.scrollBar: true
URxvt.scrollBar_right: true
URxvt.scrollstyle: plain
URxvt*scrollBar_floating: true
Urxvt.secondaryScroll: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
urxvt*font: xft:Terminess Powerline-9
URxvt.letterSpace: 0
URxvt*geometry: 150x50
! transparency
!URxvt.transparent: true
urxvt*depth: 32
!URxvt.tintColor: #2d2d2d
URxvt.shading: 40
URxvt.blurRadius: 30
! perl extensions (tabs, clickable URLs, yankable URLs)
URxvt.perl-ext:
!URxvt.perl-ext-common: default,tabbed,matcher,url-select
URxvt.perl-ext-common:
! tabs
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tabbar-fg: -2
URxvt.tabbed.tab-bg: -2
URxvt.tabbed.tab-fg: -1
URxvt.tabbed.title-bg: -1
URxvt.tabbed.title-fg: -2
URxvt.tabbed.autohide: true
URxvt.tabbed.new-button: false
URxvt.tabbed.title: true
! clickable URLs
URxvt.matcher.button: 1
URxvt.urlLauncher: /usr/bin/xdg-open
! yankable URLs
URxvt.underlineURLs: true
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
 
! fonts
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

