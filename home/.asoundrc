# vim:set ft=alsaconf:

ctl.equal0 {
	type equal;
	controls "/home/calarco/.alsaequal0.bin";
}

pcm.plugequal0 {
	type equal;
	slave.pcm "plughw:0,0";
	controls "/home/calarco/.alsaequal0.bin";
}

pcm.equal0 {
	type plug;
	slave.pcm plugequal0;
}

ctl.equal1 {
	type equal;
	controls "/home/calarco/.alsaequal1.bin";
}

pcm.plugequal1 {
	type equal;
	slave.pcm "plughw:1,7";
	controls "/home/calarco/.alsaequal1.bin";
}

pcm.equal1 {
	type plug;
	slave.pcm plugequal1;
	ttable {
		0.0 0.5
		0.1 0.5
		1.0 0.5
		1.1 0.5
	}
}
